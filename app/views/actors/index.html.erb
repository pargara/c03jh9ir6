<table>
  <thead>
    <tr>
      <th scope="col"Id</th>
      <th scope="col">Name</th>
      <th scope="col">Bio</th>
      <th scope="col">Birth_date</th>
      <th scope="col">Birth_place</th>
      <th scope="col">Image_url</th>
      <th scope="col">Alive</th>
      <th scope="col">Death_date</th>
      <th scope="col">Death_place</th>
      <th scope="col">Created_at</th>
      <th scope="col">Updated_at</th>
    </tr>
  </thead>
  <tbody>
    <% @actors.each do |actor| %>
    <tr>
      <th><%= actor.id %></th>
      <th><%= actor.name %></th>
      <th><%= actor.bio %></th>
      <th><%= actor.birth_date %></th>
      <th><%= actor.birth_place %></th>
      <th><img src="<%= actor.image_url %>" alt="Image of <%= actor.name %>"></th>
      <th><%= actor.alive %></th>
      <th><%= actor.death_date %></th>
      <th><%= actor.death_place %></th>
      <th><%= actor.created_at %></th>
      <th><%= actor.updated_at %></th>
    </tr>
    <% end %>
  </tbody>
</table>
